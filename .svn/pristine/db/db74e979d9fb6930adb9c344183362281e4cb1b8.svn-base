package zuna.clustering.HAC.experiment;

import java.util.ArrayList;

public class ExperimentImpl implements Experiment{
	private ArrayList<String> obv = new ArrayList<String>();
	
	public ExperimentImpl(ArrayList<String> obv) {
		this.obv = obv;
	}

	
	public int getNumberOfObservations() {
		return this.obv.size();
	}

	public ArrayList<String> getObv() {
		return obv;
	}

	
	

}
