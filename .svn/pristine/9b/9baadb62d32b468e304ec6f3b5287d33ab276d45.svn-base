<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Show Metric"
            visible="true"
            id="Refactoring.actionSet">
         <menu
               label="AutoRefactoring"
               id="AutoRefactoring">
            <separator
                  name="AutoRefactoring">
            </separator>
         </menu>
         <action
               class="zuna.refactoring.ui.actions.ClassDecomposer"
               icon="icons/sample.gif"
               id="Refactoring.action1"
               label="&amp;ClassDecomposer"
               menubarPath="AutoRefactoring/ClassDecomposer"
               style="push"
               toolbarPath="ClassDecomposer"
               tooltip="ClassDecomposer">
         </action>
      </actionSet>
   </extension>
   
   
   <extension
         point="org.eclipse.ui.views">
      <view
            category="Refactoring.category1"
            class="zuna.refactoring.ui.views.classDecomposesrViews.Combo.MethodMetricView"
            icon="icons/sample.gif"
            id="refactoring.views.MethodMetricView"
            name="DetailedView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="Refactoring.category1"
            class="zuna.refactoring.ui.views.classDecomposesrViews.MethodView"
            icon="icons/sample.gif"
            id="refactoring.views.MethodView"
            name="MethodView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="Refactoring.category1"
            class="zuna.refactoring.ui.views.classDecomposesrViews.ClassListView"
            icon="icons/sample.gif"
            id="refactoring.views.classDecomposesrViews.ClassListView"
            name="ClassList"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="zuna.refactoring.ui.views.DecomposedResultView"
            id="zuna.refactoring.ui.views.DecomposedResultView"
            name="DecompositionResult"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="zuna.refactoring.ui.views.classDecomposesrViews.graph.ResultViewer"
            id="zuna.refactoring.ui.views.classDecomposesrViews.GraphicalDecomposisionResultViewer"
            name="GraphicView"
            restorable="true">
      </view>
   </extension>

</plugin>
